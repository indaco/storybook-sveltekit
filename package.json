{
	"name": "storybook-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "svelte-kit sync && vitest run",
		"test:watch": "svelte-kit sync && vitest watch",
		"coverage": "svelte-kit sync && vitest run --coverage",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"plop": "plop",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@babel/core": "^7.19.3",
		"@storybook/addon-actions": "^7.0.0-rc.5",
		"@storybook/addon-essentials": "^7.0.0-rc.5",
		"@storybook/addon-interactions": "^7.0.0-rc.5",
		"@storybook/addon-links": "^7.0.0-rc.5",
		"@storybook/addon-mdx-gfm": "^7.0.0-rc.5",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^3.0.0-next.3",
		"@storybook/svelte": "^7.0.0-rc.5",
		"@storybook/sveltekit": "^7.0.0-rc.5",
		"@storybook/testing-library": "^0.0.14-next.1",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "1.12.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"babel-loader": "^8.2.5",
		"c8": "^7.12.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-storybook": "^0.6.4",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^20.0.1",
		"plop": "^3.1.1",
		"postcss": "^8.4.17",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.0.0-rc.5",
		"svelte": "3.57.0",
		"svelte-check": "3.1.4",
		"svelte-loader": "3.1.7",
		"svelte-preprocess": "5.0.3",
		"tailwindcss": "3.2.7",
		"tslib": "^2.3.1",
		"typescript": "^4.9.5",
		"vite": "4.1.4",
		"vitest": "latest"
	},
	"type": "module"
}
